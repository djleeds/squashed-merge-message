{
    "name": "Squashed Merge Message Template",
    "description": "Browser Extension to apply a squash & merge commit message template",
    "version": "0.1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [ "https://github.com/lampo/debit*" ],
            "run_at": "document_end",
            "js": [ "content.js" ]
        }
    ],
    "applications": {
      "gecko": {
        "id": "squashed-merge-message-template@damonbauer"
      }
    }
}
